  },
  "ava": {
    "babel": "inherit",
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justsml/escape-from-callback-mountain.git"
  },
  "keywords": [
    "promises",
    "bluebird",
    "javascript",
    "javascript-guide",
    "functional-js",
    "functional-patterns"
  ],
  "author": "Dan Levy <Dan@DanLevy.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justsml/escape-from-callback-mountain/issues"
  },
  "homepage": "https://github.com/justsml/escape-from-callback-mountain#readme",
  "dependencies": {
    "ava": "^0.19.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "jscs": "^3.0.7",
    "minio": "^3.1.3",
    "mongoose": "^4.11.3",
    "nedb": "^1.8.0",
    "sequelize": "^4.4.2",
    "sqlite3": "^3.1.8",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0"
  }
